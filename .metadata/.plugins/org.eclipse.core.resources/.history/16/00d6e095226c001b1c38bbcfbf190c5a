package lconvexHall;

import java.util.*;

public class GiftWrapping {
	
	public void convexHall(Coordinate point[],int n) {
		Stack<Coordinate> stack =new Stack<Coordinate>();
		
		int left= 0;         
		
		int p;
		int q;
		
		for(int i=1; i<n;i++) {
			if(point[i].x <point[left].x) {
				left=i;
				
			}
		}
		p=left;
		do {
			
		}while(p!=left);
	}

}
